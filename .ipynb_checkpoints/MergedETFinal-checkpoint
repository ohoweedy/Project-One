     "execution_count": 75,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "allsports_rel_mean = all_sports_df[\"RelPayPerYear\"].mean()\n",
    "all_sports_df[\"DevCalcOne\"] = all_sports_df[\"RelPayPerYear\"] - allsports_rel_mean\n",
    "all_sports_df[\"DevCalcTwo\"] = all_sports_df[\"DevCalcOne\"] * all_sports_df[\"DevCalcOne\"]\n",
    "all_rel_var = all_sports_df[\"DevCalcTwo\"].mean()\n",
    "all_rel_dev = math.sqrt(all_rel_var)\n",
    "all_rel_dev"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 77,
   "metadata": {},
   "outputs": [
